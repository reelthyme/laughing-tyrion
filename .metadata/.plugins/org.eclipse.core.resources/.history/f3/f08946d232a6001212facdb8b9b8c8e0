	public QuadTreeNode(float cx,float cy, float height, float width, QuadTreeNode<Collidable> parent)
	{
		this.parent=parent;
		this.cx=cx;
		this.cy=cy;
		this.height=height;
		this.width=width;
		elements=new ArrayList<Collidable>(0);
	}
	
	public boolean insert(Collidable piece){
		
		return true;
	}
	
	
	
	
}
